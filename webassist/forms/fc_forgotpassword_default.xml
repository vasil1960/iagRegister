<wa_form name="Forgot Password" id="ForgotPassword" submit_text="Send" send_mail="false" use_upload="false">
	<wa_form_fieldset label="Forgot Password" id="Forgot Password">
		<wa_fieldset_description>Required *</wa_fieldset_description>
		<wa_form_line>
			<wa_form_group_reference label="" continueLine="false">
				<wa_field_group name="">
					<wa_field_line>
						<wa_field_column>
							<wa_field_set_validation>Please enter a value.</wa_field_set_validation>
							<wa_field_reference src="text_medium.xml" label="Email" validation="{ required: true, type: 'email' }" serverValidation="email" undefinedundefined continueLine="false"></wa_field_reference>
						</wa_field_column>
					</wa_field_line>
				</wa_field_group>
			</wa_form_group_reference>
		</wa_form_line>
		<wa_form_line>
			<wa_form_group_reference group="Security.xml" src="Security.xml" label="" continueLine="false"></wa_form_group_reference>
		</wa_form_line>
	</wa_form_fieldset>
</wa_form>